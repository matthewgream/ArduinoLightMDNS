class TXTRecord {
public:
    // RFC 6763 Section 6.4 - Key names must be printable ASCII
    static constexpr size_t KEY_LENGTH_MAX = 9;      // RFC recommendation
    static constexpr size_t VALUE_LENGTH_MAX = 255;  // DNS limitation
    static constexpr size_t TOTAL_LENGTH_MAX = 255;  // Per TXT string

    struct Entry {
        String key;
        std::vector<uint8_t> value;
        bool binary;  // Indicates if value needs special encoding
    };

private:
    std::vector<Entry> entries;
    mutable uint16_t cached_length{0};
    mutable bool length_valid{false};

    static bool isValidKeyChar(char c) {
        // RFC 6763 Section 6.4
        return (c >= 0x20 && c <= 0x7E) && c != '=';
    }

    bool validateKey(const String& key) const {
        if (key.isEmpty() || key.length() > KEY_LENGTH_MAX) 
            return false;
            
        return std::all_of(key.begin(), key.end(), isValidKeyChar);
    }

public:
    Status addEntry(const String& key, const void* value = nullptr, 
                   size_t value_len = 0, bool is_binary = false) {
        if (!validateKey(key))
            return InvalidArgument;

        // Check if key already exists
        auto it = std::find_if(entries.begin(), entries.end(),
            [&key](const Entry& e) { return e.key.equalsIgnoreCase(key); });
        
        if (it != entries.end()) {
            // Update existing entry
            it->value.assign(static_cast<const uint8_t*>(value),
                           static_cast<const uint8_t*>(value) + value_len);
            it->binary = is_binary;
        } else {
            // Add new entry
            entries.push_back({
                key,
                std::vector<uint8_t>(static_cast<const uint8_t*>(value),
                                   static_cast<const uint8_t*>(value) + value_len),
                is_binary
            });
        }

        length_valid = false;
        return Success;
    }

    // Convenience methods for common types
    Status addString(const String& key, const String& value) {
        return addEntry(key, value.c_str(), value.length());
    }

    Status addFlag(const String& key) {
        return addEntry(key); // Value-less entry
    }

    Status addInt(const String& key, int value) {
        char buf[12];
        snprintf(buf, sizeof(buf), "%d", value);
        return addEntry(key, buf, strlen(buf));
    }

    uint16_t encodedLength() const {
        if (!length_valid) {
            cached_length = 0;
            for (const auto& entry : entries) {
                size_t entry_len = entry.key.length();
                if (!entry.value.empty()) {
                    entry_len += 1; // '='
                    entry_len += entry.binary ? 
                        Base64.encodedLength(entry.value.size()) : 
                        entry.value.size();
                }
                
                if (entry_len > TOTAL_LENGTH_MAX) {
                    // Handle overflow - split into multiple TXT records
                    cached_length += (entry_len / TOTAL_LENGTH_MAX + 1);
                } else {
                    cached_length += 1; // Length byte
                }
            }
            length_valid = true;
        }
        return cached_length;
    }

    void writeTo(UDP* udp) const {
        for (const auto& entry : entries) {
            String encoded;
            encoded.reserve(TOTAL_LENGTH_MAX);
            encoded += entry.key;
            
            if (!entry.value.empty()) {
                encoded += '=';
                if (entry.binary) {
                    // Base64 encode binary values
                    size_t b64_len = Base64.encodedLength(entry.value.size());
                    char* b64_buf = new char[b64_len + 1];
                    Base64.encode(b64_buf, 
                                (char*)entry.value.data(), 
                                entry.value.size());
                    encoded += b64_buf;
                    delete[] b64_buf;
                } else {
                    encoded += String((char*)entry.value.data(), 
                                    entry.value.size());
                }
            }

            // Split if necessary
            for (size_t pos = 0; pos < encoded.length(); 
                 pos += TOTAL_LENGTH_MAX) {
                size_t chunk_len = std::min(encoded.length() - pos, 
                                          (size_t)TOTAL_LENGTH_MAX);
                UDP_WRITE_BYTE(chunk_len);
                UDP_WRITE_DATA((uint8_t*)encoded.c_str() + pos, chunk_len);
            }
        }
    }
};

